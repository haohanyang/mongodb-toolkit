{
  "name": "mongodb-toolkit",
  "version": "0.1.0-beta.4",
  "description": "MongoDB utilities for data import/export, schema analysis and more",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haohanyang/mongodb-toolkit.git"
  },
  "author": "Haohan Yang",
  "license": "SSPL",
  "homepage": "https://github.com/haohanyang/mongodb-toolkit#readme",
  "bugs": {
    "url": "https://github.com/haohanyang/mongodb-toolkit/issues"
  },
  "scripts": {
    "build-lib": "rollup -c",
    "build-lib:production": "NODE_ENV=production rollup -c",
    "build-examples": "tsc -p ./tsconfig-examples.json",
    "format": "prettier --write \"*.js\" \"src/**/*.js\" \"vendors/**/*.ts\" \"src/**/*.ts\""
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.8.1",
    "@types/json-schema": "^7.0.15",
    "@types/reservoir": "^0.1.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "mongodb": "^6.14.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mongodb-schema": "^12.6.3",
    "papaparse": "^5.3.2",
    "stream-json": "^1.7.5",
    "strip-bom-stream": "^4.0.0"
  },
  "keywords": [
